<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
	<PreferenceCategory android:title="Display Settings">
		<ListPreference
		    android:key="display_list"
		    android:title="Display"
		    android:summary="@string/display_summary"
		    android:entries="@array/display_entries"
		    android:entryValues="@array/display_entryvalues"
		/>
	</PreferenceCategory>
	
	<PreferenceCategory android:title="Linefeed Code Setting">
		<ListPreference
		    android:key="linefeedcode_list"
		    android:title="Linefeed Code"
		    android:summary="@string/linefeedcode_summary"
		    android:entries="@array/linefeedcode_entries"
		    android:entryValues="@array/linefeedcode_entryvalues"
		/>
	</PreferenceCategory>
	
	<PreferenceCategory android:title="Baudrate Setting" >
        <ListPreference
            android:entries="@array/baudrate_entries"
            android:entryValues="@array/baudrate_entryvalues"
            android:key="baudrate_list"
            android:summary="@string/baudrate_summary"
            android:title="Baudrate" />
    </PreferenceCategory>
	
</PreferenceScreen>